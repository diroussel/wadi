{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "composite": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["ES2023"],
    "module": "node16",
	"moduleResolution": "node16",
    "noImplicitAny": true,
    "outDir": "target/tsc-build",
    "skipLibCheck": true,
    "strict": true,
    "target": "ES2022",
  },
  "include": [
    "./src",
  ]
}
