{
  "name": "wadi",
  "componentNames": [
    "wadi"
  ],
  "description": "The pipeline util",
  "version": "0.4.0",
  "private": true,
  "sideEffects": false,
  "exports": {
    "import": "./target/bundle/wadi.mjs",
    "require": "./target/bundle/wadi.js"
  },
  "bin": {
    "wadi": "target/bundle/wadi.js"
  },
  "files": [
    "target/bundle"
  ],
  "scripts": {
    "clean": "rm -rf target dist",
    "build": "tsup",
    "test:unit": "jest",
    "cover:unit": "pnpm run test:unit --coverage",
    "lint": "pnpm lint:js && pnpm lint:fmt",
    "lint:js": "eslint --cache .",
    "lint:fmt": "prettier --check .",
    "fix": "pnpm prettier -w . && pnpm eslint --quiet --fix .",
    "prepare": "pnpm build",
    "package": "./scripts/package.sh"
  },
  "dependencies": {
    "@aws-sdk/client-ssm": "3.379.1",
    "@aws-sdk/client-s3": "3.379.1",
    "@aws-sdk/signature-v4-crt": "3.378.0",
    "@aws-sdk/types": "3.378.0",
    "glob": "^10.3.3",
    "node-stream-zip": "^1.15.0",
    "p-map": "^6.0.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "^18.16.0",
    "@types/yargs": "^17.0.9",
    "@swc/core": "^1.3.71",
    "@swc/jest": "^0.2.27",
    "jest": "^29.6.2",
    "typescript": "^5.1.6",
    "tsup": "^7.1.0"
  },
  "packageManager": "pnpm@8.6.10"
}
